{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "open source universal react redux graphql boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.server.js && webpack --config webpack.client.js",
    "serve": "node build/server_bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec node build/server_bundle.js"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.3",
    "concurrently": "3.5.0",
    "express": "^4.16.3",
    "express-http-proxy": "1.0.6",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "1.12.0",
    "react": "16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "^2.2.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "redux-logger": "^3.0.6",
    "webpack-merge": "^4.1.4"
  }
}
